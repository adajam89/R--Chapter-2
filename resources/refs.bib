@article{algarabel2012,
  title = {Recognition Memory Deficits in Mild Cognitive Impairment},
  author = {Algarabel, Salvador and Fuentes, Manuel and Escudero, Joaqu{\'i}n and Pitarque, Alfonso and Peset, Vicente and Maz{\'o}n, Jos{\'e}-Francisco and Mel{\'e}ndez, Juan-Carlos},
