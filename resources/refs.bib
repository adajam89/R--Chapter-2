  title = {Episodic Reconstruction Contributes to High-Confidence False Recognition Memories in Older Adults: {{Evidence}} from Event-Related Potentials},
  shorttitle = {Episodic Reconstruction Contributes to High-Confidence False Recognition Memories in Older Adults},
