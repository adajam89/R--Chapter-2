@article{ahmed2008,
  title = {Naming of Objects, Faces and Buildings in Mild Cognitive Impairment},
  author = {Ahmed, Samrah and Arnold, Robert and Thompson, Sian A. and Graham, Kim S. and Hodges, John R.},
  year = {2008},
  month = jun,
  volume = {44},
