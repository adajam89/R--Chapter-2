@article{ally2009a,
  ids = {ally2009c},
  title = {The Picture Superiority Effect in Patients with {{Alzheimer}}'s Disease and Mild Cognitive Impairment},
  author = {Ally, Brandon A. and Gold, Carl A. and Budson, Andrew E.},
  date = {2009-01},
  journaltitle = {Neuropsychologia},
  shortjournal = {Neuropsychologia},
  volume = {47},
  pages = {595--598},
  issn = {00283932},
  doi = {10.1016/j.neuropsychologia.2008.10.010},
  url = {https://linkinghub.elsevier.com/retrieve/pii/S0028393208004144},
  urldate = {2020-01-17},
  abstract = {The fact that pictures are better remembered than words has been reported in the literature for over 30 years. While this picture superiority effect has been consistently found in healthy young and older adults, no study has directly evaluated the presence of the effect in patients with Alzheimer’s disease (AD) or mild cognitive impairment (MCI). Clinical observations have indicated that pictures enhance memory in these patients, suggesting that the picture superiority effect may be intact. However, several studies have reported visual processing impairments in AD and MCI patients which might diminish the picture superiority effect. Using a recognition memory paradigm, we tested memory for pictures versus words in these patients. The results showed that the picture superiority effect is intact, and that these patients showed a similar benefit to healthy controls from studying pictures compared to words. The findings are discussed in terms of visual processing and possible clinical importance.},
  file = {/Users/home/Zotero/storage/229P6WIA/Ally et al. - 2009 - The picture superiority effect in patients with Al.pdf;/Users/home/Zotero/storage/2P6282HX/Ally et al. - 2009 - The picture superiority effect in patients with Al.pdf},
  langid = {english},
  number = {2}
}

