  title = {Pictorial Superiority Effects in Oldest-Old People},
  author = {Cherry, Katie E. and Hawley, Karri S. and Jackson, Erin M. and Volaufova, Julia and Su, L. Joseph and Jazwinski, S. Michal},
  date = {2008-10},
